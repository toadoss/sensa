{
  "manifest_version": 2,

  "name": "Sensa",
  "description": "This extension highlihts sensational words given a rating",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
	"default_popup": "popup.html"
  },
  "permissions": [
    "activeTab"
  ],
   "background": {
    "scripts": ["myscript.js"],
    "persistent": false
  }
}
